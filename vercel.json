{
    "builds": [{
      "src": "config/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.12" }
    },     {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "config/wsgi.py"
      }
    ]
}